{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "compile-protos": "mkdir -p ./src/proto && npx grpc_tools_node_protoc --plugin=protoc-gen-js=./node_modules/.bin/grpc_tools_node_protoc_js_plugin --js_out=import_style=commonjs,binary:./src -I=.. ../proto/utils.proto ../proto/family_tree.proto"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "google-protobuf": "^3.21.4",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@tailwindcss/typography": "^0.5.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "grpc-tools": "^1.13.0",
    "postcss": "^8.5.3",
    "prettier": "^3.0.0-alpha.6",
    "tailwindcss": "^3.4.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
